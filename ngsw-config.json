{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "!/assets/material-design-icons/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    },
    {
      "name":"fonts",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**"
        ]
      } 
    },
    {
      "name": "icon-fonts",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/material-design-icons/**"
        ]
      }
    },
    {
      "name":"images",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "urls": [
          "https://material.angular.io/assets/img/examples/**"
        ]
      } 
    }
  ],
  "dataGroups": [
    {
      "name": "rest-api", 
      "urls": [
        "http://localhost:3000/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "1h",
        "timeout": "25s"
      }
    },
    {
      "name": "api-performance",
      "urls": [
        "/products"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "3d",
        "timeout": "10s",
        "strategy": "performance"
      }
    }
  ],
  "appData": {
    "versionMessage": "New version: Push notifications were added!"
  },
  "navigationUrls": [
    "/**",
    "!/**/*.*",
    "!/**/*__*",
    "!/**/*__*/**",
    "!/**/non-cached-route"
  ]
}
